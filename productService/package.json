{
  "name": "productservice",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "private": true,
  "scripts": {
    "test": "jest",
    "client:deploy": "sls client deploy --no-config-change --no-policy-change --no-cors-change",
    "client:deploy:nc": "npm run client:deploy -- --no-confirm",
    "client:build:deploy": "npm run build && npm run client:deploy",
    "client:build:deploy:nc": "npm run build && npm run client:deploy:nc",
    "swagger": "node ./app.js"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-cors": "^3.2.2",
    "@middy/http-json-body-parser": "^2.5.3",
    "aws-sdk": "^2.1182.0",
    "pg": "^8.7.3",
    "serverless-offline": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "express": "^4.18.1",
    "jest": "^28.1.3",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
